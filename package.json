{
  "name": "hydra-effection",
  "version": "0.0.1",
  "description": "Multiplex HTTP proxy using Effection - dynamically spawns Express servers per hostname",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "tsx start.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "effection",
    "proxy",
    "http",
    "multiplex",
    "express",
    "structured-concurrency"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "effection": ">=4.0.0-beta.0",
    "express": ">=4.0.0",
    "http-proxy": ">=1.0.0"
  },
  "dependencies": {
    "express": "^5.0.0",
    "http-proxy": "^1.18.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@effectionx/process": "^0.6.1",
    "@effectionx/tinyexec": "^0.3.1",
    "@types/express": "^5.0.0",
    "@types/http-proxy": "^1.17.17",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.0.0",
    "effection": "^4.0.0-beta.3",
    "tsup": "^8.3.5",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0"
  }
}
